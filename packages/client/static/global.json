              "attribute_name": "price",
              "value": 99,
              "comparison": "less_than"
            }
          ],
          "permissions": {
            "community": true,
            "other": false
          }
        },
        {
          "description": "Lower price",
          "action": "lower_price",
          "while_carried": false,
          "conditions": [
            {
              "attribute_name": "price",
              "value": 1,
              "comparison": "greater_than"
            }
          ],
          "permissions": {
            "community": true,
            "other": false
          }
        },
        {
          "description": "Collect gold",
